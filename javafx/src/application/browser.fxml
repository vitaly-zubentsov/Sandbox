<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.web.WebView?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.browserController">
   <top>
      <BorderPane BorderPane.alignment="CENTER">
         <right>
            <Button mnemonicParsing="false" onAction="#onButtonCLick" prefHeight="25.0" prefWidth="107.0" text="Download" BorderPane.alignment="CENTER" />
         </right>
         <center>
            <TextField fx:id="urlTextField" prefHeight="25.0" prefWidth="271.0" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </top>
   <center>
      <WebView fx:id="browserWebView" prefHeight="398.0" prefWidth="381.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
